{
	"root": true,
	"extends": ["marine/prettier/node", "plugin:jsdoc/recommended"],
	"plugins": ["jsdoc"],
	"parserOptions": {
		"project": "./tsconfig.eslint.json"
	},
	"rules": {
		"jsdoc/require-param-type": "off",
		"jsdoc/require-returns": "off",
		"jsdoc/require-returns-type": "off",
		"@typescript-eslint/naming-convention": [
			"error",
			{
				"selector": "enumMember",
				"format": ["PascalCase"]
			}
		]
	}
}
